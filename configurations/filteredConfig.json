{
  "start_avd": [
    {
      "table": "worker_current_states",
      "method": "upsert",
      "key_columns": [
        "worker_id"
      ],
      "mappings": {
        "worker_id": "details.node_id + '_' + details.user_id + '_' + details.instance_id",
        "request_id": "details.request_id",
        "event_time": "NOW",
        "state_code": "event",
        "details": "FULL_JSON",
        "status": "operation"
      }
    },
    {
      "table": "avd_operations",
      "method": "insert",
      "mappings": {
        "avd_name": "details.instance_id",
        "user": "details.user_id",
        "node": "details.node_id",
        "operation_time": "NOW"
      }
    }
  ],
  "activate_whatsapp": [
    {
      "table": "whatsapp_activations",
      "method": "update",
      "key_columns": [
        "req_id"
      ],
      "mappings": {
        "req_id": "details.request_id",
        "phone_number": "details.phone_number",
        "profile_name": "details.profile_name",
        "inserted_at": "NOW"
      }
    },
    {
      "table": "worker_current_states",
      "method": "upsert",
      "key_columns": [
        "worker_id"
      ],
      "mappings": {
        "worker_id": "details.node_id + '_' + details.user_id + '_' + details.instance_id",
        "request_id": "details.request_id",
        "event_time": "NOW",
        "state_code": "event",
        "details": "FULL_JSON",
        "status": "operation"
      }
    }
  ]
}